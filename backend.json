{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z0-9]*$": {
      "type": "object",
      "properties": {
        "dob": {
          "type": "string",
          "format": "date"
        },
        "name": {
          "type": "string"
        },
        "photo": {
          "type": "string",
          "format": "uri"
        },
        "insta": {
          "type": "string"
        },
        "snap": {
          "type": "string"
        }
      },
      "required": ["dob", "name", "photo", "snap", "insta"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
